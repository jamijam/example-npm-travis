{
  "name": "example-npm-travis",
  "version": "1.0.0",
  "description": "A premium Vue.js admin dashboard for bootstrap by Scantist",
  "author": "Scantist pte ltd",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/lint-staged && sh version_hook.sh"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,vue}": [
      "git add"
    ],
    "*.vue": [
      "node_modules/.bin/prettyhtml",
      "git add"
    ]
  },
  "scripts": {
    "dev": "cross-env node build/dev-server.js",
    "test-server": "cross-env NODE_ENV=testserver node build/dev-server.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "build-staging": "cross-env NODE_ENV=staging node build/build.js",
    "build-onsite": "cross-env NODE_ENV=onsite node build/build.js",
    "build-enterprise": "cross-env NODE_ENV=enterprise node build/build.js",
    "e2e": "node test/e2e/runner.js",
    "test": "jest --silent",
    "snapshot-update": "jest --silent -u",
    "start": "yarn build-enterprise && NODE_ENV=enterprise node server.js"
  },
